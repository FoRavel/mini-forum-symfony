<nav aria-label="...">
        <ul class="pagination">
            <li class="page-item disabled">
                <a class="page-link" href="" tabindex="-1">Previous</a>
            </li>
            {% for i in 0..pagination.pages %}
            {% if i != 0 %}
            <li class="page-item">
                <a class="page-link" href="{{path}}">{{ i }}</a>
            </li>
            {% endif %}
            
            {% endfor %}
            <li class="page-item">
                <a class="page-link" href="#">Next</a>
            </li>
        </ul>
    </nav>